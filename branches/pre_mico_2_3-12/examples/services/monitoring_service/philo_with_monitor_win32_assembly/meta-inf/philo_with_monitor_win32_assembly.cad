			<componentimplref idref="UUID-1E42D587-648D-4B2C-BD3F-A11BFF295B4F"/>
  	        <componentinstantiation id="atze">
				<componentproperties>
					<fileinarchive name="meta-inf/atze.cpf"/>
				</componentproperties>
	        </componentinstantiation>
	        <componentinstantiation id="bob">
