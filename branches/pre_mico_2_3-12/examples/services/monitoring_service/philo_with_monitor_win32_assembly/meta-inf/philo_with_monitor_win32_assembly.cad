				<componentproperties>
					<fileinarchive name="meta-inf/observer.cpf"/>
				</componentproperties>	                    
			    <registercomponent><registerwithnaming name="CCM/Components/Observer"/></registercomponent>
			</componentinstantiation>
	    </homeplacement>
	</processcollocation>
